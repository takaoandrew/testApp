<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrewtakao.testapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextInputActivity" />
        <activity android:name=".AutoCompleteActivity" />
        <activity android:name=".CheckBoxActivity" />
        <activity android:name=".ToggleButtonActivity" />
        <activity
            android:name=".SpinnerActivity"
            android:label="@string/title_activity_spinner"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".ListViewActivity"
            android:label="@string/pictures_label"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.andrewtakao.testapp.MainActivity" />
        </activity>
        <activity android:name=".TimePickerActivity"></activity>
    </application>

</manifest>